import java.util.Scanner;
public class Consonante {
public static void main(String args[]) {
    Scanner teclado = new Scanner(System.in);

    System.out.println("Escriba una cadena para separar vocales.");
    String x = teclado.next();
    int s = x.length();
    char k;
        for (int i = 0; i < s; i++) {
            k = x.charAt(i);
            if ((k != 'a') || (k != 'e') || (k != 'i') || (k != 'o')
                    || (k != 'u')) {
                k = ' ';
            }
            System.out.print(k);
        }
    }
}