import java.util.*;
public class Calculos {

	public static void main(String[] args) {
		int num1, num2, res;
		Scanner in = new Scanner(System.in);
		System.out.println("Intrdozca los valos de la operación");
			System.out.println("Primer valor: ");
			num1 = in.nextInt();
			System.out.println("Segundo valor: ");
			num2 = in.nextInt();
			res= num1 + num2;
		System.out.println("El resultado de la operación " + num1 +  " y "  + num2 + " es " + res);
	}

}
