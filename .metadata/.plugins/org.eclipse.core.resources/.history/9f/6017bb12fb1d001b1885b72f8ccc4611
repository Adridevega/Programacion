	Scanner in= new Scanner(System.in);
	public static void main(String[] args) {
		int[] posicion= {1, 2, 4, 6, 5, 0, 7};
		String[] nombres = {"Juan" , "Ana" , "escribe" , "diferente" , "carta" , "que" , "mejor" , "."};
		int pos;
		int i;
		// Ana escrube cartas mejor que Juan
		//1-2-3-6-5-0-7
		for(i= 0; i < posicion.length; i++) {
			pos= posicion[i];
			System.out.println(nombre[]);
		}
	}

}
