import java.util.*;
public class While {

	public static void main(String[] args) {
		Scanner in = new Scanner (System.in);
		int num;
		int i = 1;
			System.out.println("Introduzca el nuúmero del cual quieres saber la tabla de multiplicar: ");
			num= in.nextInt();
				
		while (i <= 10) {
			System.out.println(num +  "*" + i + "=" + num * i);
			i++;
			
		}
		
	}

}
