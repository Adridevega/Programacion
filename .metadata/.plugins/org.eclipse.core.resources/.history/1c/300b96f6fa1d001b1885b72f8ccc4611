
public class Arrays {

	public static void main(String[] args) {
		//se han creado 6 variables de numeros entero de una vez
		//donde pone "int" se cambia por cualquier variable para que deje de ser entero
		int[] paguinas = new int[6];
		//para acceder a cada uno seria:
		paguinas[0] = 20;
		paguinas[1] = 50;
		System.out.println(paguinas[1]);
		//recordar que en los arrays el primer numero siempre sera 0, siendo que si son 6 variables seria del 0 al 5
		//Para hacer una arrays de caracteres ser deberá usar un String
		String cadena = "Holaaaaa";
		System.out.println(cadena);	
	}

}
