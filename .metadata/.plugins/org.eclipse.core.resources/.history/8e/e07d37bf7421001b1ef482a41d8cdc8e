package paqueteDam;
import java.util.*;
public class Ejercicio02_1 {

	public static void main(String[] args) {
		int num1=0;
		int num2=0;
		int res;
		int resto;
	
		Scanner in=new Scanner(System.in);
		System.out.println("Introduzca el nuúmero  ");
		num1= in.nextInt();
		System.out.println("Introduzca el nuúmero  ");
		num2= in.nextInt();
		res=num1/num2;
		resto=num1%num2;
		System.out.println(res);
		}

}
