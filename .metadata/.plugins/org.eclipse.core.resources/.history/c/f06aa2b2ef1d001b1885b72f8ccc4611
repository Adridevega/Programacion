import java.util.*;
public class Operadores_Logicos {

	public static void main(String[] args) {
		Scanner in= new Scanner (System.in);
		//los numeros premiados son 4,5 y 20
		int numero;
		System.out.println("Introduzca su número para ver si ha sido premiado");
		numero = in.nextInt();
		if (numero == 4 || numero == 5 || numero == 20) {
			System.out.println("El numero es el premiado");
		}
		else {
			System.out.println("No es ninguno de los numero premiados");
		}
	}

}
